<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="eva_art" extends="struts-default">
		<action name="login" class="action.user.authen.UserLoginAction">
			<result name="success">/success.jsp</result>
			<result name="error">/fail.jsp</result>
		</action>
		<action name="register" class="action.user.register.UserRegisterAction">
			<result name="success">/success.jsp</result>
			<result name="error">/fail.jsp</result>
		</action>
		<action name="updatePassword" class="action.user.update.UserUpdatePasswordAction">
			<result name="success">/passwordUpdateSuccess.jsp</result>
			<result name="error">/fail.jsp</result>
		</action>
	</package>
</struts>